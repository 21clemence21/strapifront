"line":43,"column":24}},{"generated":{"line":88,"column":0},"source":"js/component/library/bookList.js","original":{"line":53,"column":21}},{"generated":{"line":88,"column":3},"source":"js/component/library/bookList.js","original":{"line":39,"column":48}},{"generated":{"line":88,"column":4},"source":"js/component/library/bookList.js","original":{"line":39,"column":20}},{"generated":{"line":88,"column":20},"source":"js/component/library/bookList.js","original":{"line":53,"column":25}},{"generated":{"line":88,"column":74},"source":"js/component/library/bookList.js","original":{"line":37,"column":12}},{"generated":{"line":88,"column":75},"source":"js/component/library/bookList.js","original":{"line":26,"column":8}},{"generated":{"line":88,"column":76},"source":"js/component/library/bookList.js","original":{"line":25,"column":4}},{"generated":{"line":89,"column":0},"source":"js/component/library/bookList.js","original":{"line":58,"column":1}},{"generated":{"line":89,"column":1},"source":"js/component/library/bookList.js","original":{"line":5,"column":0}},{"name":"BookList","generated":{"line":91,"column":15},"source":"js/component/library/bookList.js","original":{"line":60,"column":15}}],"sources":{"js/component/library/bookList.js":"import React, { useEffect, useState } from 'react';\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { getBooks } from '../../store/bookStore';\r\n\r\nconst BookList = () => {\r\n    const dispatch = useDispatch();\r\n    const books = useSelector(state => state.books.collection)\r\n    const booksAreLoading = useSelector(state => state.books.isLoading)\r\n    const [currentFilter, setCurrentFilter] = useState('');\r\n\r\n    //Une fois que mon app est init, je peux aller récupérer la liste de tous les livres\r\n    //Quand ?\r\n    //Quand le composant (App) se render la première fois et uniquement la première fois pour ne pas faire plusieurs call d'API\r\n    useEffect(() => {\r\n        dispatch(getBooks());\r\n    }, [])\r\n\r\n    if (booksAreLoading) return <span className=\"f1 font-bold text-green\">Fetching books...</span>\r\n\r\n    const handleFilter = (event) => {\r\n        dispatch(getBooks(event.target.value));\r\n        setCurrentFilter(event.target.value);\r\n    }\r\n\r\n    return (\r\n        <section>\r\n\r\n            <header>\r\n                <select value={currentFilter} onChange={handleFilter}>\r\n                    <option value={''}>Tous</option>\r\n                    <option value={'?_sort=Name:ASC'}>Titre alphabetique</option>\r\n                    <option value={'?_sort=created_at:ASC'}>Date de création</option>\r\n                    <option value={'?borrowed=false'} >Livre(s) disponible(s)</option>\r\n                </select>\r\n            </header>\r\n\r\n            <ul className=\"flex flex-col\">\r\n                {\r\n                    books && books.length > 0 ? books.map(book => {\r\n\r\n                        const date = new Date(book.created_at);\r\n\r\n                        return (\r\n                            <li className=\"\" key={book.id}>\r\n                                <div className=\"p-4\">\r\n                                    <span className=\"font-bold uppercases f4\">{book.Name}</span>\r\n                                    <p className=\"lh-4\">{book.Resume}</p>\r\n                                    <span className=\"text-blue font-bold\">{`Date d'ajout : ${date.toLocaleDateString('fr-FR')}`}</span>\r\n                                </div>\r\n                                <hr className=\"m-0\" />\r\n                            </li>\r\n                        )\r\n                    }) : <span>No books</span>\r\n                }\r\n            </ul>\r\n        </section>\r\n    )\r\n}\r\n\r\nexport default BookList\r\n"},"lineCount":null}},"error":null,"hash":"1be54af107e54fb61329cdbd9ef0fc07","cacheData":{"env":{}}}